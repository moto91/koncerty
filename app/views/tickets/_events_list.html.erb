<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Artist</th>
      <th>Price range</th>
      <th>Event description</th>
      <th>Event date</th>
      <th>Event ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% events.each do |event| %>
      <tr>
        <td><%= event.artist %></td>
        <td><%= event.price_D.to_s + " - " + event.price_GC.to_s %></td>
        <td><%= event.description %></td>
        <td><%= event.event_date %></td>
        <td><%= event.id %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
